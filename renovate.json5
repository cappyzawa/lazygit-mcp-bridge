{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
    ':semanticCommitTypeAll(deps)',
  ],
  timezone: 'Asia/Tokyo',
  schedule: [
    'before 4am on monday',
  ],
  commitMessagePrefix: 'deps:',
  commitMessageAction: 'update',
  commitMessageTopic: '{{depName}}',
  commitMessageExtra: 'to {{newVersion}}',
  semanticCommits: 'enabled',
  addLabels: [
    'dependencies',
  ],
  platformAutomerge: true,
  automerge: true,
  automergeType: 'pr',
  automergeStrategy: 'squash',
  packageRules: [
    {
      matchManagers: [
        'gomod',
      ],
      matchUpdateTypes: [
        'minor',
        'patch',
      ],
      automerge: true,
      automergeType: 'pr',
      ignoreTests: true,
    },
    {
      matchManagers: [
        'gomod',
      ],
      matchUpdateTypes: [
        'major',
      ],
      automerge: false,
      addLabels: [
        'major-update',
      ],
    },
    {
      matchManagers: [
        'github-actions',
      ],
      matchUpdateTypes: [
        'minor',
        'patch',
      ],
      automerge: true,
      automergeType: 'pr',
      addLabels: [
        'github-actions',
      ],
    },
    {
      matchManagers: [
        'github-actions',
      ],
      matchUpdateTypes: [
        'major',
      ],
      automerge: false,
      addLabels: [
        'major-update',
        'github-actions',
      ],
    },
    {
      matchManagers: [
        'dockerfile',
      ],
      matchUpdateTypes: [
        'minor',
        'patch',
      ],
      automerge: true,
      automergeType: 'pr',
    },
    {
      matchManagers: [
        'gomod',
      ],
      groupName: 'Go dependencies',
      groupSlug: 'go-deps',
    },
    {
      matchManagers: [
        'github-actions',
      ],
      groupName: 'GitHub Actions',
      groupSlug: 'github-actions',
    },
  ],
  gomod: {
    enabled: true,
  },
  'github-actions': {
    enabled: true,
    managerFilePatterns: [
      '/\\.github/workflows/.*\\.ya?ml$/',
    ],
  },
  vulnerabilityAlerts: {
    enabled: true,
    automerge: true,
    schedule: [
      'at any time',
    ],
  },
  prHourlyLimit: 5,
  prConcurrentLimit: 3,
  branchConcurrentLimit: 10,
  ignoreDeps: [],
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/^go\\.mod$/',
      ],
      matchStrings: [
        'go (?<currentValue>\\d+\\.\\d+)',
      ],
      datasourceTemplate: 'golang-version',
      depNameTemplate: 'go',
    },
  ],
}
